{
  "name": "olimpiadas-web",
  "version": "1.0.0",
  "description": "Olimpiadas Web",
  "main": "server.js",
  "scripts": {
    "clear": "rm -rf build && mkdir build",
    "docker": "docker-compose up -d",
    "start": "npm run clear && cp ./app/properties.yaml ./build && ./node_modules/.bin/./babel -d ./build ./app/ && node ./build/server.js",
    "start-dev": " npm run docker && ./node_modules/.bin/nodemon -e js,yaml --exec babel-node app/server.js",
    "lint": "./node_modules/.bin/eslint ./app/ --quiet",
    "test": "echo \"No test specified\" && exit 0",
    "validate": "npm run lint && npm outdated --depth 0 && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fabiohbarbosa/olimpiadas-web.git"
  },
  "author": "FÃ¡bio H. G. Barbosa",
  "license": "ISC",
  "homepage": "https://github.com/fabiohbarbosa/olimpiadas-web",
  "dependencies": {
    "express": "^4.13.3",
    "express-load": "^1.1.15",
    "feedparser": "^1.1.3",
    "iconv": "^2.1.10",
    "lodash": "^4.13.1",
    "log": "^1.4.0",
    "moment": "^2.13.0",
    "mongoose": "^4.5.3",
    "node-schedule": "^1.1.1",
    "request": "^2.63.0",
    "yamljs": "^0.2.8"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "eslint": "^3.0.0",
    "nodemon": "^1.9.2"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
